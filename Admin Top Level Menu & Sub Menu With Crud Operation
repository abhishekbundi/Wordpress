Create Menu & Sub Menu in Admin Side Wordpress
------------------------------------------------
function wp_recipiants_menu_page(){
    add_menu_page( 
        __(
         'Recipiants', 'textdomain' ),
        'Recipiants',
        'manage_options',
        'recipiants',
        'recipiants_menu_page',
        'dashicons-buddicons-buddypress-logo',
        10
    ); 

    add_submenu_page("recipiants","Import data","Import Data","manage_options","import-data","import_data");
}
add_action( 'admin_menu', 'wp_recipiants_menu_page' );
function recipiants_menu_page(){
    $page_template = include(dirname( __FILE__ ) . '/inc/recipiants.php');
    return $page_template;
}
function import_data(){
$page_template = include(dirname( __FILE__ ) . '/inc/import_data.php');
    return $page_template;
}

