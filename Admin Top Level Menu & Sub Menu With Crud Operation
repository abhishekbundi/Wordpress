<?php
global $wpdb;
if(isset($_REQUEST['save_data']))
{
  $country=$_POST['country'];
  $name=$_POST['name'];
  $function=$_POST['function'];
  $sending_date=$_POST['sending_date'];
  $answer=$_POST['answer'];
  $insert = $wpdb->query("INSERT INTO wp_recipiants(wp_recipiants.country,wp_recipiants.name,wp_recipiants.function,wp_recipiants.sendig_date,wp_recipiants.answer_received,wp_recipiants.lang) VALUES ('$country','$name','$function','$sending_date','$answer','$my_current_lang')");
}
if(isset($_REQUEST['update_data']))
{
  $country=$_POST['country'];
  $ids=$_POST['ids'];
  $name=$_POST['name'];
  $function=$_POST['function'];
  $sending_date=$_POST['sending_date'];
  $answer=$_POST['answer'];
  $inserts = $wpdb->query("UPDATE wp_recipiants SET wp_recipiants.country='$country',wp_recipiants.name='$name',wp_recipiants.function='$function',wp_recipiants.sendig_date='$sending_date',wp_recipiants.answer_received='$answer' WHERE wp_recipiants.id='$ids'");
}

if (isset($_GET['pageno'])) {
  $pageno = $_GET['pageno'];
} else {
  $pageno =1;
}
$no_of_records_per_page =50;
$offset = ($pageno-1) * $no_of_records_per_page;
$country_list=['Österreich'];

$fun_list=$wpdb->get_results("SELECT wp_recipiants.function FROM wp_recipiants GROUP by wp_recipiants.function ORDER by wp_recipiants.function ASC");

if(isset($_REQUEST['function_f']))
{
    $wq='WHERE ';
/*    $country_f=$_REQUEST['country_f'];*/
    $function_f=$_REQUEST['function_f'];
    if($country_f<>'')
    {
        $wq.='country='."'$country_f'";
    }
    if($function_f<>'')
    {
        $wq.=' function='."'$function_f'";
    }
}
if(isset($_REQUEST['id']))
{
    $id=$_REQUEST['id'];
    $insert = $wpdb->query("DELETE FROM wp_recipiants WHERE wp_recipiants.id='$id'");
}
?>
