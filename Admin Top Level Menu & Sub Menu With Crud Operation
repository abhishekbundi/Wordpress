             <thead>
                <th>Country</th>
                <th>Name</th>
                <th>Function</th>
                <th>Sendig date</th>
                <th>Answer received?Yes / No</th>
                <th>Action</th>
            </thead>
            <tbody>
                <?php 
                if(isset($_REQUEST['filter']))
                {
/*                    $country_f=$_POST['country_f'];*/
                    $function_f=$_REQUEST['function_f'];
                    $results=$wpdb->get_results("SELECT COUNT(*) as total_recored FROM wp_recipiants ".$wq." AND wp_recipiants.lang='$my_current_lang'");
                    $total_rows=$results[0]->total_recored;
                    $total_pages = ceil($total_rows / $no_of_records_per_page);

                    $results=$wpdb->get_results("SELECT wp_recipiants.id,wp_recipiants.country,wp_recipiants.name,wp_recipiants.function,wp_recipiants.sendig_date,wp_recipiants.answer_received,wp_recipiants.lang FROM wp_recipiants ".$wq." AND wp_recipiants.lang='$my_current_lang' ORDER by wp_recipiants.id DESC LIMIT $offset, $no_of_records_per_page"); 
                }else
                {
                    $results=$wpdb->get_results("SELECT COUNT(*) as total_recored FROM wp_recipiants WHERE wp_recipiants.lang='$my_current_lang'");
                    $total_rows=$results[0]->total_recored;
                    $total_pages = ceil($total_rows / $no_of_records_per_page);

                    $results=$wpdb->get_results("SELECT wp_recipiants.id,wp_recipiants.country,wp_recipiants.name,wp_recipiants.function,wp_recipiants.sendig_date,wp_recipiants.answer_received,wp_recipiants.lang FROM wp_recipiants WHERE wp_recipiants.lang='$my_current_lang' ORDER by wp_recipiants.id DESC LIMIT $offset, $no_of_records_per_page");
                }

                $total_data=count($results);
                foreach($results  as $key => $row) {
                   ?>
                   <tr>
                      <td><?php echo $row->country; ?></td>
                      <td><?php echo $row->name; ?></td>
                      <td><?php echo $row->function; ?></td>
                      <td><?php echo $row->sendig_date;?></td>
                      <td><?php echo $row->answer_received; ?></td>
                      <td>
                        <button class="btn btn-warning edit_data" id="<?php echo $row->id.','.$row->country.','.$row->name.','.$row->function.','.$row->sendig_date.','.$row->answer_received; ?>"><span class="fa fa-pencil"></span></button>
                        <a href="admin.php?page=recipiants&id=<?php echo $row->id; ?>" class="btn btn-danger" onclick="return confirm('Are you sure you want to Remove?');"><span class="fa fa-trash"></span></a>
                    </td>
                </tr>
            <?php } ?>
        </tbody>
    </table>
    <div class="page_nation">
        <?php if($total_data>=1){ ?>
          <ul class="pagination">
            <li class="page-item"><a href="admin.php?page=recipiants&pageno=1" class="searchPagination">«</a></li>
            <li class="<?php if($pageno <= 1){ echo 'disabled'; } ?> page-item">
              <a href="<?php if($pageno <= 1){ echo '#'; } else { echo "&pageno=".($pageno - 1); } ?>" class="searchPagination"><</a>
              </li>
              <?php
              for($i =$pageno-1; $i<= $pageno+1; $i++) {  
                if($i<=$total_pages&&$i>=1){
                  if($i==$pageno){$aclass='active';}
                  echo '<li class="page-item">';
                  echo '<a href = "admin.php?page=recipiants&pageno=' . $i . '" class="searchPagination '.$aclass.'">' . $i . ' </a>';  
                  echo '</li>';
              }
              $aclass='';
          }    
          ?>
          <li class="<?php if($pageno >= $total_pages){ echo 'disabled'; } ?> page-item">
            <a href="<?php if($pageno >= $total_pages){ echo '#'; } else { echo "admin.php?page=recipiants&pageno=".($pageno + 1); } ?>" class="searchPagination" >></a>
        </li>
        <li class="page-item"><a href="admin.php?page=recipiants&pageno=<?php echo $total_pages; ?>" class="searchPagination">»</a></li>
    </ul>
<?php }else
{ ?>
    <div class="l-row-restricted">
      <div class="toolbar">
        <p class="empty">There are no results matching</p>
    </div>
</div>
<?php }
?>
</div>


