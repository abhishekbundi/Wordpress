<script type="text/javascript">
    $(".edit_data").click(function(){
        var id=$(this).attr('id');
        const e_data= id.split(",");
        $("#name").val(e_data[2]);
        $("#sending_date").val(e_data[4]);
        $("#id").val(e_data[0]);
        $('#country').val(e_data[1]).change();
        $('#function').val(e_data[3]).change();
        $('#answer').val(e_data[5]).change();
        $("#edit_data").modal('show');
    })
</script>