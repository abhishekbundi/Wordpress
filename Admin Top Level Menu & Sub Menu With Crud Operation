            <?php } ?>
        </select> -->
        <select name="function_f" class="form-select">
            <option value="">Select a Function</option>
            <?php foreach($fun_list  as $key => $fl){?>
                <option value="<?php echo $fl->function;?>" <?php if($fl->function==$function_f){echo 'selected';} ?> ><?php echo $fl->function; ?></option>
            <?php } ?>
        </select>
        <input type="submit" name="filter" value="Filter" class="btn btn-primary">
        <a href="admin.php?page=recipiants"><button type="button" class="btn btn-secondary">Filter löschen</button></a>
    </div>

    <div class="col-md-4">
